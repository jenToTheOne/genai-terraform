# Application Gateway Configuration Example
# Copy this to terraform.tfvars and customize

project_name     = "genai-project"
location        = "Southeast Asia"
environment     = "dev"
appgw_sku_name  = "Standard_v2"
appgw_sku_tier  = "Standard_v2"
appgw_capacity  = 2

# Network Configuration
vnet_address_space      = ["10.3.0.0/16"]
subnet_address_prefixes = ["10.3.1.0/24"]
vnet_name              = "appgw-vnet"
subnet_name            = "appgw-subnet"

tags = {
  Environment = "dev"
  Project     = "GenAI-AppGW"
  Owner       = "DevOps-Team"
  CostCenter  = "IT"
}
